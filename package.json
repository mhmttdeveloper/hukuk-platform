{
  "name": "yargi-tam",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "cd apps/web && npm run dev",
    "build": "cd apps/web && npm run build",
    "start": "cd apps/web && npm run start",
    "lint": "cd apps/web && npm run lint",
    "db:generate": "cd apps/web && npm run db:generate",
    "db:migrate": "cd apps/web && npm run db:migrate",
    "db:seed": "cd apps/web && npm run db:seed",
    "db:studio": "cd apps/web && npm run db:studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
